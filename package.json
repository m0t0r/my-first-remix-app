{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.ts,.tsx --ignore-path .gitignore --fix",
